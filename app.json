{
  "expo": {
    "name": "kovzy",
    "slug": "kovzy",
    "version": "1.0.1",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon-dark.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "enabled": true,
      "url": "https://u.expo.dev/7c64e824-e18a-4864-bfc6-5399fa7da11a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kovzy.app",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "NOTIFICATIONS",
        "BACKGROUND_FETCH",
        "VIBRATE",
        "WAKE_LOCK",
        "REQUEST_IGNORE_BATTERY_OPTIMIZATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "FOREGROUND_SERVICE",
        "MODIFY_AUDIO_SETTINGS",
        "ACCESS_NOTIFICATION_POLICY",
        "SCHEDULE_EXACT_ALARM"
      ],
      "jsEngine": "hermes",
      "package": "com.kovzy.app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7c64e824-e18a-4864-bfc6-5399fa7da11a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      "expo-secure-store",
      "expo-updates",
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": ["./src/assets/audio/plucky.mp3"],
          "defaultChannel": "kovzyOrders",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-asset",
      "expo-font"
    ]
  }
}
